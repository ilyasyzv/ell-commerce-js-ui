{
  "name": "@pearson-ell/commerce-ui",
  "version": "1.0.28",
  "description": "ELL eCommerce components library for react",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "eslint --fix ./src && rollup -c",
    "lint": "eslint --fix ./src",
    "init": "npm i && npm link ../ell-commerce-web-app/node_modules/react",
    "dev": "del /q dist && rollup -c -w",
    "build:rollup": "rollup -c",
    "build": "del /q dist && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "storybook": "set NODE_OPTIONS=--openssl-legacy-provider && start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "Micro FE",
  "license": "MIT",
  "types": "dist/esm/index.d.ts",
