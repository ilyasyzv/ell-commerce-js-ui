{
  "name": "ell-commerce-ui",
  "version": "1.0.9",
  "description": "ELL eCommerce components library for react",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "init": "npm i && npm link ../ell-commerce-web-app/node_modules/react",
    "dev": "del /q dist && rollup -c -w",
    "build:rollup": "rollup -c",
    "build": "del /q dist && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "storybook": "set NODE_OPTIONS=--openssl-legacy-provider && start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "Micro FE",
  "license": "MIT",
  "types": "dist/esm/index.d.ts",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-image": "^3.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack4": "^6.5.16",
    "@storybook/manager-webpack4": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@svgr/rollup": "^6.5.1",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "axios": "^1.2.2",
    "babel-loader": "^8.3.0",
    "ell-commerce-sdk": "^1.1.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.10.0",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-ignore": "^1.0.10",
    "rollup-plugin-img": "^1.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.34.1",
    "rollup-pluginutils": "^2.8.2",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "ell-commerce-sdk": "^1.1.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "html-react-parser": "^3.0.4",
    "styled-components": "^5.3.6"
  }
}
